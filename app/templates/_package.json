{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prettier": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0"
  <% } %>},
  "devDependencies": {
    "eslint": "^4.6.0",
    "eslint-config-precure": "^0.8.0",
    "jest": "^20.0.4",
    "prettier": "^1.6.1"
  }
}
